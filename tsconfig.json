{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "dom",
      "es6",
      "dom.iterable",
      "scripthost",
      "es2019"
    ],
    "jsx": "react",
    "strict": true,
    "types": [
      "node",
      "webpack-env",
      "jest"
    ],
    "esModuleInterop": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ]
}
